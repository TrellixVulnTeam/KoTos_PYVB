<script type="text/javascript">

	function showNavToday(show) {
		var mainshow = "nav_"+show;
		var aNav = $$('#navArea .tNavBlock');
		for (var i = 0; i < aNav.length; i++) {
			if (aNav[i].id == mainshow) {
				aNav[i].show();
			} else {
				aNav[i].hide();
			}
		}
	}

	function outNavToday (current, active) {
		if (current != active) {
			id = "nav_"+current;
			$(id).style.display = "none";
			id = "nav_"+active;
			$(id).style.display = "block";
		}
	}

</script>


        
				 
